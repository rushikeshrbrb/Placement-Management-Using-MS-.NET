@{
}


@model List<WebApplicationIdentity.Models.JobApplication>

<h3>Recieved Job Applications</h3>

<table class="table table-hover table-bordered">
    <thead>
        <tr class="table-primary">
            <th>Student Name</th>
            <th>Email</th>
            <th>Mobile No</th>
            <th>Experience</th>
            <th>Graduation Year</th>
            <th>Skills</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var jobApplication in Model)
        {
            <tr>
                <td>@jobApplication.Student.StudentName</td>
                <td>@jobApplication.Email</td>
                <td>@jobApplication.MobileNo</td>
                <td>@jobApplication.Experience</td>
                <td>@jobApplication.GraduationYear</td>
                <td>@jobApplication.skills</td>
                <td>@jobApplication.Status</td>
            </tr>
        }
    </tbody>
</table>
