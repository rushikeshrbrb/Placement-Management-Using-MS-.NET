@{
}
@model IEnumerable<WebApplicationIdentity.Models.JobApplication>

<h1>List of Job Applications</h1>

<table class="table table-hover table-bordered">
    <thead>
        <tr class="table-primary">
            <th>Application Id</th>
            <th>Email</th>
            <th>Mobile Number</th>
            <th>Experience</th>
            <th>Graduation Year</th>
            <th>Skills</th>
            <th>Status</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var jobApplication in Model)
        {
            <tr>
                <td>@jobApplication.ApplicationId</td>
                <td>@jobApplication.Email</td>
                <td>@jobApplication.MobileNo</td>
                <td>@jobApplication.Experience</td>
                <td>@jobApplication.GraduationYear</td>
                <td>@jobApplication.skills</td>
                <td>@jobApplication.Status</td>
                <!-- Add more columns as needed -->
            </tr>
        }
    </tbody>
</table>
